##########################
# component parameters

[component]

ns-ip 127.0.0.1
ns-port 20002

# the name of the component for the naming service
name @ROBOTINONAME@.SmartFestoMPSDocking


##########################
# server port parameter

[dockingEventServer]
serviceName dockingevent


##########################
# client port parameter

[basePushTimedClient]
serverName @ROBOTINONAME@.SmartRobotinoBaseServer
serviceName basestate
wiringName basePushTimedClient
interval 1

[irClient]
serverName @ROBOTINONAME@.SmartRobotinoIRServer
serviceName newestAdditionalIRScan
wiringName irClient

[laserClient]
serverName @ROBOTINONAME@.@LASERTYPE@
serviceName scan
wiringName laserClient

[navVelSendClient]
serverName @ROBOTINONAME@.SmartRobotinoBaseServer
serviceName navigationvelocity
wiringName navVelSendClient


#######################################
# internal parameters                ##
[docking]                            ##
blob_min_num_scan_points 2           ##Don't change
deamonIP 127.0.0.1                   ##Don't change
irDocking_Left_AI1_Multiplier 100.0  ##Don't change
irDocking_Left_AI1_Offset 0.0        ##Range -1.0 to 1.0(Offset value for the IR sensor BG4)
irDocking_Center_AI2_Multiplier 100.0##Don't change
irDocking_Center_AI2_Offset 0.0      ##Range -1.0 to 1.0(Offset value for the IR sensor BG5)
irDocking_Right_AI3_Multiplier 100.0 ##Don't change
irDocking_Right_AI3_Offset 0.2       ##Range -1.0 to 1.0(Offset value for the IR sensor BG6)
irDocking_LsRs_Diff_Tollerance 0.25  ##Don't change
irDocking_c_s_ZeroLevel 10.0         ##Range 8.0 to 20.0(Anything beyond this distance(cm) is consider as 09 for BG5)
irDocking_L1_move_X_dir 10.0         ##Range 8.0 to 15.0(Set point distance(cm) to be reached during stage1 of IR_Docking)
irDocking_L2_move_X_dir_max 8.5      ##Range 8.0 to 15.0(Max point distance(cm) to be maintained during Last stage of IR_Docking)
irDocking_L2_move_X_dir_min 8.0      ##Range 7.0 to 14.0(Min point distance(cm) to be maintained during Last stage of IR_Docking)

