##########################
# component parameters

[component]

ns-ip 127.0.0.1
ns-port 20002

# the name of the component for the naming service
name @ROBOTINONAME@.SmartRobotinoBaseServer


##########################
# server port parameter

[basePositionServer]
serviceName basestate
cycle 0.1
[basePositionUpdateServer]
serviceName positionUpdate
[baseQueryServer]
serviceName basestatequery
[batteryEventServer]
serviceName battery
[bumperEventServer]
serviceName bumperEventServer
[digitalInputEventServer]
serviceName digitalInputEventServer
[iOQueryServer]
serviceName iOQueryServer
[laserSafetyFieldEventServer]
serviceName laserSafetyFieldEventServer
[navigationVelocityServer]
serviceName navigationvelocity
[powerOutPutSendServer]
serviceName powerOutPutSendServer


##########################
# client port parameter

[localizationEventClient]
serverName @ROBOTINONAME@.SmartAmcl
serviceName localizationEvent
wiringName localizationEventClient


##########################
# internal parameters
[Bumper]
bumperTimeOutMSec 500
bumperTimeOutSec 0

[General]
hasSignalState true
poseFileName /tmp/lastRobotPose
writePoseFile true
useLocalizationEvent true
verbose false

[LaserSafetyField]
generateLaserSafetyFieldEvents @GENERATELASERSAFETYFIELDEVENTS@
laserSafetyfFieldTimeOutMSec 0
laserSafetyfFieldTimeOutSec 10

[Robot]

daemonIP @RPCDIP@

maxRotVel 6

maxVelX 1.5

maxVelY 1.0

